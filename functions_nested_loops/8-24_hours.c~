#include <stdlib.h>
#include "main.h"
void jack_bauer(void)
{
	int hour, minute;
	
	for (hour = 0; hour < 24; hour++)
	{
		for (minute =0; minute < 60; minute++)
		{
			_putchar((hour / 10) & (hour % 10) + '0');
			_putchar(':');
			_putchar((minute / 10) & (minute % 10) + '0');
			_putchar(10);
		}
	}
}
