#include <stdio.h>
#include "main.h"
/**
 * _strncat - function that concatenate two strings
 * @src: a pointer to a char
 * @dest: a pointer to a char
 * Return: the pointer to dest
 */
char *_strncat(char *dest, char *src, int n)
{
	int i, j;

	for (i = 0; dest[i] <!= '\0'; i++)
		;
	for (j = 0; src[j] && j < n; j++)
	{
		dest[i] = src[j];
		i++;
	}
	return (dest);
}
