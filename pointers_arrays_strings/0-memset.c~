#include "main.h"
/**
 * _memset - fills memory with constant byte
 * @s: memory area
 * @b: constant byte b
 * @n: first n bytes of memory area pointed to by s
 * Return: string (success)
 **/

char *_memset(char *s, char b, unsigned int n)
{

	unsigned int a;

	for (a = 0; a < n; i++)
	{
		s[a] = b;
	}
	return (s);
}
